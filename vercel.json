{
    "routes": [
      {
        "src": "/static/(.*)",           
        "dest": "/static/$1"
      },
      {
        "src": "/assets/(.*)",           
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*\\.js)",
        "dest": "/$1"
      },
      {
        "src": "/(.*\\.css)",
        "dest": "/$1"
      },
      {
        "src": "/(.*\\.ico)",
        "dest": "/$1"
      },
      {
        "src": "/(.*\\.png)",
        "dest": "/$1"
      },
      {
        "src": "/editor/(?<roomId>[^/]+)",
        "dest": "/index.html"
      },
      {
        "src": "/login",
        "dest": "/index.html"
      },
      {
        "src": "/register",
        "dest": "/index.html"
      },
      {
        "src": "/createRoom",
        "dest": "/index.html"
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }
  